	   <hardwareInterface>EffortJointInterface</hardwareInterface>
       <!-- hardwareInterface>VelocityJointInterface</hardwareInterface -->
       <mechanicalReduction>${wheel_mechanical_reduction}</mechanicalReduction>
       <!-- motorTorqueConstant>1</motorTorqueConstant -->
     </actuator>     
  </transmission>


  <!-- FRONT RIGHT WHEEL (GREY) -->
  <link name="front_right_wheel">
    <inertial>
      <mass value="4.5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia  ixx="0.059" ixy="0.0"  ixz="0.0"  iyy="0.055"  iyz="0.0"  izz="0.059" />
    </inertial>

    <visual>
       <origin xyz="0 0 0" rpy="0 0 0" /> 
       <geometry>
         <mesh filename="package://guardian_description/meshes/wheel.dae"/>
       </geometry>
	<material name="grey">
		<color rgba="0.3 0.3 0.3 1"/>
      	</material>
    </visual>
    <collision>
       <origin xyz="0 0 0" rpy="0 0 0" /> 
       <geometry>
         <mesh filename="package://guardian_description/meshes/wheel.dae"/> 
       </geometry>
    </collision>
  </link>

  <joint name="joint_front_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel"/>
    <origin xyz="${wheel_offset_x} -${wheel_offset_y} 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="${wheel_joint_effort_limit}" velocity="${wheel_joint_velocity_limit}"/>
    <joint_properties damping="${wheel_joint_damping}" friction="{wheel_joint_friction}"/>
  </joint>


  <transmission name="joint_frw_trans">
	 <type>transmission_interface/SimpleTransmission</type>
     <joint name="joint_front_right_wheel" >
  <hardwareInterface>EffortJointInterface</hardwareInterface>
</joint>
     <actuator name="joint_frw_motor">
	   <hardwareInterface>EffortJointInterface</hardwareInterface>
	   <!-- hardwareInterface>VelocityJointInterface</hardwareInterface -->    
       <mechanicalReduction>${wheel_mechanical_reduction}</mechanicalReduction>
       <!-- motorTorqueConstant>1</motorTorqueConstant -->
     </actuator>
     <!-- motorTorqueConstant>1</motorTorqueConstant -->
  </transmission>



  <!-- WAM ARM -->
 
  <link name="wam_link1">
<inertial>
		  <mass value="2.3936"/>
		  <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
		  <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
		</inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry name="wam_link1_visual">
        <mesh filename="robot/iri_wam/iri_wam_description/mesh/wam1.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="White">
        <color rgba="1.0 1.0 1.0 1.0"/>
      </material>
    </visual>
<collision>
		  <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
		  <geometry>
			<mesh filename="package://robotnik_wam_description/meshes/wam1.stl"/>
		  </geometry>
		  <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
		</collision>
  </link>
  <joint name="j1_joint" type="revolute">
    <parent link="base_link"/>
    <child link="wam_link1"/>
    <origin xyz="0.125 0 0.55" rpy="0 0 0" />
    <axis xyz="0 0 1"/>
    <limit effort="30" lower="-2.6" upper="2.6" velocity="2.0"/>
    <dynamics damping="100"/>
  </joint>

<transmission name="j_1_transmission" >
	     <type>transmission_interface/SimpleTransmission</type>
	     <actuator name="j_1_joint_motor">
	          <hardwareInterface>EffortJointInterface</hardwareInterface>
		  <!-- hardwareInterface>VelocityJointInterface</hardwareInterface -->
                  <mechanicalReduction>1</mechanicalReduction>
     	          <!-- motorTorqueConstant>1</motorTorqueConstant -->
             </actuator>
	     <joint name="j1_joint">
                 <hardwareInterface>EffortJointInterface</hardwareInterface>
             </joint>
	</transmission>	


